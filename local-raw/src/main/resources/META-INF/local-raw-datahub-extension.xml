<extension xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.hybris.com/schema/"
		   xsi:schemaLocation="http://www.hybris.com/schema/ http://www.hybris.com/schema/datahub-metadata-schema-1.3.0.xsd"
		   name="local-raw">

	<dependencies>
		<dependency>
			<extension>local-canonical</extension>
		</dependency>
	</dependencies>

	<rawItems>
		<item>
			<type>RawProduct</type>
			<description>Raw representation of apparel product</description>
			<attributes>
				<attribute>
					<name>baseName</name>
				</attribute>
				<attribute>
					<name>baseDescription</name>
				</attribute>
				<attribute>
					<name>ean</name>
				</attribute>
				<attribute>
					<name>manufacturerAID</name>
				</attribute>
				<attribute>
					<name>unit</name>
				</attribute>
				<attribute>
					<name>numberContentUnits</name>
				</attribute>
				<attribute>
					<name>category</name>
				</attribute>
				<attribute>
					<name>style</name>
				</attribute>
				<attribute>
					<name>styleDescription</name>
				</attribute>
				<attribute>
					<name>size</name>
				</attribute>
				<attribute>
					<name>SKU</name>
				</attribute>
				<attribute>
					<name>sourceId</name>
				</attribute>
			</attributes>
		</item>
		<item>
			<type>RawCategory</type>
			<description>Raw representation of apparel category</description>
			<attributes>
				<attribute>
					<name>name</name>
				</attribute>
				<attribute>
					<name>description</name>
				</attribute>
				<attribute>
					<name>parent</name>
				</attribute>
				<attribute>
					<name>sourceId</name>
				</attribute>
			</attributes>
		</item>
	</rawItems>

	<canonicalItems>
		<item>
			<type>CanonicalProduct</type>
			<attributes>
				<attribute>
					<name>identifier</name>
					<transformations>
						<transformation>
							<rawSource>RawProduct</rawSource>
							<expression>sourceId</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>baseName</name>
					<transformations>
						<transformation>
							<rawSource>RawProduct</rawSource>
							<expression>baseName</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>baseDescription</name>
					<transformations>
						<transformation>
							<rawSource>RawProduct</rawSource>
							<expression>baseDescription</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>ean</name>
					<transformations>
						<transformation>
							<rawSource>RawProduct</rawSource>
							<expression>ean</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>manufacturerAID</name>
					<transformations>
						<transformation>
							<rawSource>RawProduct</rawSource>
							<expression>manufacturerAID</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>unit</name>
					<transformations>
						<transformation>
							<rawSource>RawProduct</rawSource>
							<expression>unit</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>numberContentUnits</name>
					<transformations>
						<transformation>
							<rawSource>RawProduct</rawSource>
							<expression>numberContentUnits</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>category</name>
					<transformations>
						<transformation>
							<rawSource>RawProduct</rawSource>
							<expression>category</expression>
						</transformation>
					</transformations>
				</attribute>
			</attributes>
		</item>
		<item>
			<type>CanonicalVariant</type>
			<description>Canonical representation of apparel variant</description>
			<status>ACTIVE</status>
			<attributes>
				<attribute>
					<name>identifier</name>
					<transformations>
						<transformation>
							<rawSource>RawProduct</rawSource>
							<expression>SKU</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>product</name>
					<transformations>
						<transformation>
							<rawSource>RawProduct</rawSource>
							<expression>sourceId</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>size</name>
					<transformations>
						<transformation>
							<rawSource>RawProduct</rawSource>
							<expression>size</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>style</name>
					<transformations>
						<transformation>
							<rawSource>RawProduct</rawSource>
							<expression>style</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>styleDescription</name>
					<transformations>
						<transformation>
							<rawSource>RawProduct</rawSource>
							<expression>styleDescription</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>SKU</name>
					<transformations>
						<transformation>
							<rawSource>RawProduct</rawSource>
							<expression>SKU</expression>
						</transformation>
					</transformations>
				</attribute>
			</attributes>
		</item>
		<item>
			<type>CanonicalCategory</type>
			<description>Canonical representation of apparel category</description>
			<status>ACTIVE</status>
			<attributes>
				<attribute>
					<name>identifier</name>
					<transformations>
						<transformation>
							<rawSource>RawCategory</rawSource>
							<expression>sourceId</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>name</name>
					<transformations>
						<transformation>
							<rawSource>RawCategory</rawSource>
							<expression>name</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>description</name>
					<transformations>
						<transformation>
							<rawSource>RawCategory</rawSource>
							<expression>description</expression>
						</transformation>
					</transformations>
				</attribute>
				<attribute>
					<name>parent</name>
					<transformations>
						<transformation>
							<rawSource>RawCategory</rawSource>
							<expression>parent</expression>
						</transformation>
					</transformations>
				</attribute>
			</attributes>
		</item>

	</canonicalItems>


</extension>
